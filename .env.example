# Example .env file for packing-api
# Copy this file to .env and replace the placeholder values with secure secrets.

JWT_SECRET=replace_this_with_a_strong_base64_or_random_value
JWT_EXPIRES_IN=1h

# Admin account seed. Provide either ADMIN_PASSWORD (plain) or ADMIN_PASSWORD_HASH (bcrypt hash).
# For security, prefer ADMIN_PASSWORD_HASH in production.
ADMIN_USERNAME=admin
ADMIN_PASSWORD=replace_with_a_strong_password_at_least_8_chars
ADMIN_PASSWORD_HASH=
